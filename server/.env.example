# 服务器配置示例
# 请复制此文件为.env并根据需要修改

# 服务器端口
SERVER_PORT=:8080

# Gin运行模式 (debug, release, test)
GIN_MODE=debug

# mysql数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=123456
DB_NAME=student_management
# JWT配置
JWT_SECRET_KEY=your-secret-key-change-in-production
JWT_TOKEN_EXPIRY=24
JWT_REFRESH_EXPIRY=168
JWT_ISSUER=student-management-system
